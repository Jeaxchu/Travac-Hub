local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/istillplaying2/Travac-Hub/main/UI%20Library"))()

local s = Library:Window("  Travac Hub | Kill Monsters to Save Princess V1", "Travac Hub V1", 0, true) -- Create Window

local a = s:Tab("  Main | Auto Farm", true, 1.3)

a:Toggle("  Auto Join", function(state)
    if state then
        _G.Print = true
        while _G.Print do
        shared.Enabled = true
if game.Players.LocalPlayer.Character.Humanoid.Health > 100 then
    game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Battle"):WaitForChild("JoinPrepare"):FireServer()

while true do
wait(0.5)
if game.Players.LocalPlayer.Character.Humanoid.Health < 1 then
    wait(3)
      game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Battle"):WaitForChild("JoinPrepare"):FireServer()
end
end
end
        wait()
    end
    else
        _G.Print = false
        shared.Enabled = false
        game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Battle"):WaitForChild("ExitPrepare"):FireServer()
        end
end)

a:Toggle("  Auto Kill", function(state)
    if state then
        _G.Print = true
        while _G.Print do
        shared.Enabled = true
           game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Weapon"):WaitForChild("TakeDamage"):FireServer()
        wait()
    end
    else
        _G.Print = false
        shared.Enabled = false
        end
end)
